{
  "$schema": "https://www.krakend.io/schema/krakend.json",
  "version": 3,
  "name": "krakend_instal",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "port": 4444,
  "endpoints": [
    {
      "endpoint": "/user-agent",
      "method": "GET",
      "output_encoding": "no-op",
      "backend": [
        {
          "url_pattern": "/user-agent",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://httpbin.io"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}
